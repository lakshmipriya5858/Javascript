<p id="p1">initial content</p>
<input type="button" value="un shift" onclick="unshifting()"/>
<input type="button" value="populate array content" onclick="readArrayContent()"/>
<script>
var elements=[100,"hello",89,90.8,true,56];
function readArrayContent()
{
document.getElementById("p1").innerHTML=elements.join(",");
}
function unshifting()
{
var element=elements.unshift("hello");
alert(element);
readArrayContent();
}
//unshift: adding first element
</script>